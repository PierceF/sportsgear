<h1>Gears</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item.photos %></td>
        <td><%= item.name %></td>
        <td><%= item.price %></td>
        <td><%= link_to 'Show', item %></td>
      </tr>
    <% end %>

    <% @deals.each do |deal| %>
      <tr>
        <td><%= deal.item.photos %></td>
        <td><%= deal.item.name %></td>
        <td><%= deal.item.price %></td>

      </tr>
    <% end %>


  </tbody>
</table>

