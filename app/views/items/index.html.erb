<h1>Items#index</h1>
<p>Find me in app/views/items/index.html.erb</p>
<thead>
    <tr>
      <th>Name</th>
      <th>Banner url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item.photos %></td>
        <td><%= link_to 'Show', item %></td>

      </tr>
    <% end %>
  </tbody>
</table>

